generator client {
  provider = "prisma-client-js"
  previewFeatures = ["postgresqlExtensions"]
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
  extensions = [citext]
}

model User {
  id    String     @id @default(cuid()) @db.VarChar(32)
  email String  @unique @db.Citext()
  password  String?
}
